<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_mypage"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="hanjo.simukgak.MypageActivity">

    <Button
        android:text="OrderLog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="82dp"
        android:id="@+id/OrderLogButton"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="49dp"
        android:layout_marginStart="49dp" />

    <Button
        android:text="DutchLog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/DutchLogButton"
        android:layout_alignBaseline="@+id/OrderLogButton"
        android:layout_alignBottom="@+id/OrderLogButton"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="63dp"
        android:layout_marginEnd="63dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/DietButton"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/OrderLogButton"
        android:layout_alignStart="@+id/OrderLogButton"
        android:layout_marginBottom="118dp"
        android:text="Diet" />

    <Button
        android:text="AddressChange"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/AddressChangeButton"
        android:layout_alignBaseline="@+id/DietButton"
        android:layout_alignBottom="@+id/DietButton"
        android:layout_alignLeft="@+id/DutchLogButton"
        android:layout_alignStart="@+id/DutchLogButton" />
</RelativeLayout>
