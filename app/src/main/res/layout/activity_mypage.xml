<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/back"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="hanjo.simukgak.Mypage">


    <Button
        android:text="주문 내역"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/OrderlistButton"
        android:onClick="onClickOrderlist"
        android:layout_margin="10dp"
        android:textColor="#F5C30B"
        android:background="#000000"/>

    <Button
        android:text="주문 식당 랭킹"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/ManageButton"
        android:onClick="onClickManage"
        android:layout_margin="10dp"
        android:textColor="#F5C30B"
        android:background="#000000"
        />

    <Button
        android:text="주소지 변경"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/AddressButton"
        android:onClick="onClickAddress"
        android:layout_margin="10dp"
        android:textColor="#F5C30B"
        android:background="#000000"/>
</LinearLayout>
